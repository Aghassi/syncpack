{
  "name": "syncpack",
  "description": "Normalise differences in packages/*/package.json files in Lerna Monorepos",
  "version": "0.3.1",
  "author": "Jamie Mason <jamie@foldleft.io> (https://github.com/JamieMason)",
  "bin": {
    "syncpack": "dist/bin.js",
    "syncpack-sync-versions": "dist/bin-sync-versions.js",
    "syncpack-copy-values": "dist/bin-copy-values.js"
  },
  "bugs": "https://github.com/JamieMason/syncpack/issues",
  "dependencies": {
    "@types/fs-extra": "5.0.0",
    "@types/globby": "6.1.0",
    "@types/jest": "22.1.0",
    "@types/lodash": "4.14.97",
    "@types/mock-fs": "3.6.30",
    "@types/node": "9.4.0",
    "@types/semver": "5.4.0",
    "chalk": "2.3.0",
    "commander": "2.13.0",
    "fs-extra": "5.0.0",
    "globby": "7.1.1",
    "lodash": "4.17.4",
    "semver": "5.5.0"
  },
  "devDependencies": {
    "expect-more-jest": "1.0.1",
    "jest": "22.1.4",
    "mock-fs": "4.4.2",
    "prettier": "1.10.2",
    "rimraf": "2.6.2",
    "ts-jest": "22.0.2",
    "tslint": "5.9.1",
    "typescript": "2.6.2"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/JamieMason/syncpack#readme",
  "keywords": [],
  "license": "MIT",
  "main": "syncpack",
  "repository": "JamieMason/syncpack",
  "scripts": {
    "build": "tsc --project .",
    "format": "prettier --write '**/*.md' './src/**/*.ts' './test/**/*.ts'",
    "lint": "tslint --fix --project ./tsconfig.json",
    "prebuild": "rimraf dist ./test/dist",
    "prelint": "npm run format",
    "prepack": "npm run build",
    "test": "jest"
  }
}
